{
  "name": "agri-acoustic-sentinel",
  "version": "1.0.0",
  "description": "Bioacoustic monitoring system for crop health using Replicate AI",
  "main": "server/index.js",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "cd server && npm run dev",
    "client": "cd client && npm start",
    "install-all": "npm install && cd server && npm install && cd ../client && npm install",
    "start": "npm run server"
  },
  "keywords": [
    "agriculture",
    "bioacoustics",
    "pest-detection",
    "ai",
    "replicate",
    "crop-monitoring"
  ],
  "author": "Agri-Acoustic Sentinel Team",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "dependencies": {
    "axios": "^1.12.2",
    "form-data": "^4.0.4"
  }
}
